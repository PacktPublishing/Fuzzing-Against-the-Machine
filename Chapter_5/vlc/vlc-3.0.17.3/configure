    { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: dmgbuild not found -- unable to build fancy DMGs" >&5
$as_echo "$as_me: WARNING: dmgbuild not found -- unable to build fancy DMGs" >&2;}

fi
fi
